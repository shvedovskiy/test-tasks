  },
  "scripts": {
    "dev:start": "npx nodemon",
    "dev:wds": "rimraf dist && cross-env NODE_ENV=development npx webpack-dev-server --progress",
    "prod:build": "npx rimraf lib dist && npx babel src -d lib --ignore .test.js && cross-env NODE_ENV=production npx webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production npx pm2 start lib/server && npx pm2 logs",
    "prod:stop": "npx pm2 delete server",
    "lint": "npx eslint src webpack.config.js --ext .js,.jsx && npx flow",
    "test": "npm run lint && npx jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
