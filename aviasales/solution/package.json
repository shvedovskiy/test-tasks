{
  "name": "aviasales-test-task",
  "version": "0.0.1",
  "description": "Search Application",
  "keywords": [
    "react",
    "pwa",
    "styled-components",
    "code-splitting"
  ],
  "homepage": "https://github.com/shvedovskiy/test-tasks/tree/master/aviasales",
  "license": "WTFPL",
  "author": "Oleg Shvedov",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shvedovskiy/test-tasks"
  },
  "scripts": {
    "dev:start": "cross-env NODE_ENV=development nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx && flow",
    "test": "npm run lint && jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.14.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.5.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-seamless-immutable": "^0.4.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.5.4",
    "seamless-immutable": "^7.1.3",
    "uuid": "^3.2.1",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.0.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-flow-react-proptypes": "^24.0.1",
    "babel-plugin-inline-json-import": "^0.2.1",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.4",
    "directory-named-webpack-plugin": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.73.0",
    "jest": "^23.0.1",
    "nodemon": "^1.17.5",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "pm2": "^2.10.4",
    "postcss-loader": "^2.0.6",
