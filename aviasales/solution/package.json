  "scripts": {
    "dev:start": "npx nodemon",
    "dev:wds": "rimraf dist && npx cross-env NODE_ENV=development npx webpack-dev-server --progress",
    "build": "npx rimraf lib dist && npx babel src -d lib --ignore .test.js && npx cross-env NODE_ENV=production npx webpack -p --progress",
    "start": "npx cross-env NODE_ENV=production node lib/server",
    "lint": "npx eslint src webpack.config.js --ext .js,.jsx --fix",
    "test": "npm run lint && npx jest --coverage",
    "test:watch": "npx jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
